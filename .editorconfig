# Settings for code editor
# Skip if you have editor from JetBrains, BBEdit, Coda or SourceLair
#
# Otherwise go to http://editorconfig.org and download plugin for your code editor.

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
